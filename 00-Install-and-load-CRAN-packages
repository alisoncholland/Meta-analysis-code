# Install and load MAc, metafor and dplyr CRAN packages
# -----------------------------------------------------

# install packages
install.packages("MAc")
install.packages("metafor")
install.packages("dplyr")
install.packages("Hmisc")
install.packages("ggplot")

# load packages
library(MAc)
library(metafor)
library(dplyr)
library(Hmisc)
library(ggplot)
